                indexPageUrl: "@Url.Action("Index")", 
                buttonsContainerSelector: ".form-commands-container" 
            }); 
             
            $('#Roles_CataTab-tabs a[data-toggle="tab"]').on('shown.bs.tab', function (e, s) { 
                var tabShown = e.target.attributes["href"].value; 

                if (tabShown == "#roles_Funcionalidad_Acciones_TransTab") { 
                    setupRoles_Funcionalidad_Acciones_TransGrid(); 
                } 
                if (tabShown == "#persona_Roles_TransTab") { 
                    setupPersona_Roles_TransGrid(); 
                } 
            });             
            $("#Roles_CataTab-tabs a[data-toggle='tab']:first").tab("show"); 
        }); 

        function setupRoles_Funcionalidad_Acciones_TransGrid() { 
            var gridId = "Roles_Funcionalidad_Acciones_TransGrid"; 
 
			if ($("#" + gridId).data("grid-initialized")) 
				return; 
			else  
				$("#" + gridId).data("grid-initialized", "true"); 
 
            gridUtils.setupGridToolbarButtons({ 
                gridId: gridId, 
				toolbarContainerSelector: "#roles_Funcionalidad_Acciones_TransTab", 
                reportUrl: "@Url.Action("ReportRoles_Funcionalidad_Acciones_Trans", "Roles_Cata", new {masterId = Model.Role_Numero})" 
            }); 
 
            $("#" + gridId).dataTable({ 
                "aoColumns": [ 
                @Html.Raw(uiObject.GetGridColumnsArray("Roles_Funcionalidad_Acciones_Trans"))
                ], 
                "aaSorting": [[1, "asc"]], 
                 
                "oLanguage": datatables_locale, 
                "bFilter": false, /* Disable filtering */ 
                "bSort": false, /* Disable sorting */ 
                "bProcessing": true, 
				 
                "bServerSide": true, 
                "sAjaxSource": "@Url.Action("LoadRoles_Funcionalidad_Acciones_Trans")", 
                "sServerMethod": "POST", 
                "fnServerParams": function(aoData) { 
                    aoData.push({ "name": "masterId", "value": "@Model.Role_Numero" }); 
                } 
            }); 
		} 

        function setupPersona_Roles_TransGrid() { 
