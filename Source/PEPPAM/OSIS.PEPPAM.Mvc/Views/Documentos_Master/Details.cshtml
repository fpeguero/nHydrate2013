@section Scripts { 
	@Scripts.Render("~/bundles/jqueryval") 
    <script type="text/javascript"> 
        $(function () { 
            formUtils.setupFormActionButtons({ 
                formId: "Documentos_MasterForm", 
                indexPageUrl: "@Url.Action("Index")", 
                buttonsContainerSelector: ".form-commands-container" 
            }); 
             
            $('#Documentos_MasterTab-tabs a[data-toggle="tab"]').on('shown.bs.tab', function (e, s) { 
                var tabShown = e.target.attributes["href"].value; 

                if (tabShown == "#documentos_Objetivos_TransTab") { 
                    setupDocumentos_Objetivos_TransGrid(); 
                } 
            });             
            $("#Documentos_MasterTab-tabs a[data-toggle='tab']:first").tab("show"); 
        }); 

        function setupDocumentos_Objetivos_TransGrid() { 
            var gridId = "Documentos_Objetivos_TransGrid"; 
 
			if ($("#" + gridId).data("grid-initialized")) 
				return; 
			else  
				$("#" + gridId).data("grid-initialized", "true"); 
 
            gridUtils.setupGridToolbarButtons({ 
                gridId: gridId, 
				toolbarContainerSelector: "#documentos_Objetivos_TransTab", 
                reportUrl: "@Url.Action("ReportDocumentos_Objetivos_Trans", "Documentos_Master", new {masterId = Model.Documento_Secuencia})" 
            }); 
 
            $("#" + gridId).dataTable({ 
                "aoColumns": [ 
                @Html.Raw(uiObject.GetGridColumnsArray("Documentos_Objetivos_Trans"))
                ], 
                "aaSorting": [[1, "asc"]], 
                 
                "oLanguage": datatables_locale, 
                "bFilter": false, /* Disable filtering */ 
                "bSort": false, /* Disable sorting */ 
                "bProcessing": true, 
				 
                "bServerSide": true, 
                "sAjaxSource": "@Url.Action("LoadDocumentos_Objetivos_Trans")", 
                "sServerMethod": "POST", 
                "fnServerParams": function(aoData) { 
                    aoData.push({ "name": "masterId", "value": "@Model.Documento_Secuencia" }); 
                } 
            }); 
		} 

</script> 
