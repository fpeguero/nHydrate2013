                reportUrl: "@Url.Action("ReportPersonas_Turnos_Trans", "Horario_Turno_Trans", new {masterId = Model.Horario_Turno_Secuencia})" 
            }); 
 
            $("#" + gridId).dataTable({ 
                "aoColumns": [ 
